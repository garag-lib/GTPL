{
  "name": "gtpl",
  "version": "1.1.1",
  "type": "module",
  "repository": "https://github.com/garag-lib/GTPL",
  "description": "GTPL is a TypeScript library for reactive templates.",
  "main": "./dist/gtpl.cjs.js",
  "module": "./dist/gtpl.esm.js",
  "types": "./dist/gtpl.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/gtpl.esm.js",
      "require": "./dist/gtpl.cjs.js",
      "types": "./dist/gtpl.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment NODE_ENV:development"
  },
  "author": "Manuel Peliz",
  "license": "LGPL-2.1 license",
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}